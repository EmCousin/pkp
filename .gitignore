# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history
.yalc
yalc.lock

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

/config/master.key

/config/credentials/development.key

/config/credentials/production.key

/config/credentials/test.key
coverage

/app/assets/builds/*
!/app/assets/builds/.keep

docker-compose.yml

.env
